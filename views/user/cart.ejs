        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center clean">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col">Image</th>
                                        <th scope="col">Name</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Quantity</th>
                                        <th scope="col">Subtotal</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(productsWithQuantity && productsWithQuantity.length > 0) { %>
                                        <% for(let i = 0; i < productsWithQuantity.length; i++) { %>
                                            <% let productWithQuantity = productsWithQuantity[i]; %>
                                            <tr>
                                                <td class="image">
                                                    <img src="/public/products/<%= productWithQuantity.product.image[0] %>" alt="<%= productWithQuantity.product.name %>">
                                                </td>
                                                <td class="product-des">
                                                    <h5 class="product-name"><a href="shop-product-right.html"><%= productWithQuantity.product.name %></a></h5>
                                                    <!-- Add other product details here -->
                                                    <p><%= productWithQuantity.product.description %></p>
                                                </td>
                                                <td class="price" data-title="Price"><span> ₹ <%= parseFloat(productWithQuantity.product.offerPrice).toFixed(2) %></span></td>
                                                <td class="text-center" data-title="Quantity">
                                                    <div class="detail-qty border radius m-auto">
                                                        <a class="qty-down" onclick="updateQuantity('<%= productWithQuantity.product._id %>', 'decrease', '<%= i %>')">
                                                            <i class="fi-rs-angle-small-down"></i>
                                                        </a>
                                                        <span class="qty-val" id="quantity-<%= i %>"><%= productWithQuantity.quantity %></span>
                                                        <a class="qty-up" onclick="updateQuantity('<%= productWithQuantity.product._id %>', 'increase', '<%= i %>')">
                                                            <i class="fi-rs-angle-small-up"></i>
                                                        </a>
                                                    </div>
                                                </td>                                                
                                                <td class="text-right" data-title="Cart">
                                                    <span id="subtotal-<%= i %>"> &#8377; <%= (productWithQuantity.quantity * parseFloat(productWithQuantity.product.offerPrice)).toFixed(2) %></span>
                                                </td>
                                                <td class="action" data-title="Remove"><a onclick="deleteProduct('<%= productWithQuantity.product._id %>')" class="text-muted"><i class="fi-rs-trash"></i></a></td>
                                            </tr>
                                        <% } %>
                                    <% } else { %>
                                        <tr>
                                            <td colspan="6" class="text-end">
                                                <p>No products in the cart.</p>
                                            </td>
                                        </tr>
                                    <% } %>
                                </tbody>                             
                            </table>
                        </div>
                        <div class="cart-action text-end">
                            <a class="btn  mr-10 mb-sm-15" href="/shop"><i class="fi-rs-shuffle mr-10" ></i>Continue Shopping</a>
                            <% if(productsWithQuantity && productsWithQuantity.length > 0) { %>
                                <a href="/checkOut" class="btn "><i class="fi-rs-box-alt mr-10"></i> Proceed To
                                    CheckOut</a>
                            <% }%>

                            <script>
                                function showEmptyCartMessage() {
                                    toastr.error('Your cart is empty');
                                }
                            </script>
                            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css">

                        </div>
                        <div class="total-section">
                            <h3 style="color: #2c3e50; font-size: 20px; font-weight: bold; margin-top: 20px; margin-bottom: 10px; text-align: center; background-color: #f8f9fa; padding: 10px; border-radius: 10px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);">
                                Total Price:  ₹ <span id="total-price"><%= totalPrice %></span>
                            </h3>
                        </div>                                                
                        <div class="divider center_icon mt-50 mb-50"><i class="fi-rs-fingerprint"></i></div>

                    </div>
                    
                </div>
            </div>
        </section>
